{
	//*******************************************
	//*** map setup
	//*******************************************
	
	// uses a config object to start the setup cutscene after a delay (because the last cutscene, that changed the map is still active)
	loa2_l5_castle_of_the_chaos_wizard__spire__setup_delay: {
		event: {
			eventType: CONFIG_GENERATED_EVENT,
			stringValue: loa2_l5_castle_of_the_chaos_wizard__spire__setup,
		}
		executionType: FIRE_EVENT,
		executionParameters: {
			eventType: CONFIG_GAME_OBJECT_ACTION,
            stringValue: loa2_l5_castle_of_the_chaos_wizard__spire__config_object__start_setup_cutscene_after_delay,
		}
	},
	
	loa2_l5_castle_of_the_chaos_wizard__spire__setup: {
		event: {
			eventType: CONFIG_GENERATED_EVENT,
			stringValue: loa2_l5_castle_of_the_chaos_wizard__spire__start_setup_cutscene_after_delay,
		}
		executionType: START_CUTSCENE,
		executionParameters: {
			cutsceneId: loa2_l5_castle_of_the_chaos_wizard__spire__setup_cutscene,
		}
	},
	
	//*******************************************
	//*** event objects touched
	//*******************************************
	
	loa2_l5_castle_of_the_chaos_wizard__spire__left_side: {
		event: {
			eventType: EVENT_OBJECT_TOUCHED,
			stringValue: loa2_l5_castle_of_the_chaos_wizard__spire__left_side,
		}
		executionType: START_CUTSCENE,
		executionParameters: {
			cutsceneId: loa2_l5_castle_of_the_chaos_wizard__spire__change_side_left_cutscene,
		}
	},
	
	loa2_l5_castle_of_the_chaos_wizard__spire__right_side: {
		event: {
			eventType: EVENT_OBJECT_TOUCHED,
			stringValue: loa2_l5_castle_of_the_chaos_wizard__spire__right_side,
		}
		executionType: START_CUTSCENE,
		executionParameters: {
			cutsceneId: loa2_l5_castle_of_the_chaos_wizard__spire__change_side_right_cutscene,
		}
	},
}